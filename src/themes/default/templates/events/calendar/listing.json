{
	'calendar' : {
		'id' : {{calendar.id}},
		'title' : '{{calendar.name}}',
		'url' : 'http://{{request.get_host}}{{calendar.get_absolute_url}}'
	},
	'start': "{{start|date:"r"}}",
	'end': "{{end|date:"r"}}",
	'event_count': {{events|length}},
	'events': [
	{%for event in events%}
		{
			'event_id' : {{event.event.id}},
			'instance_id' : {{event.id}},
			'title' : '{{event.title}}',
			'description' : '{{event.description}}',
			'start' : '{{event.start|date:"r"}}',
			'end' : '{{event.end|date:"r"}}'
		},
	{%endfor%}
	]
}