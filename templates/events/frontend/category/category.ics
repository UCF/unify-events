BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//UNIFY
{% for event_instance in events %}
BEGIN:VEVENT
UID:http://{{ request.get_host }}{{ event_instance.get_absolute_url }}
DTSTAMP:{{ event_instance.start|date:"Ymd\THis" }}
DTSTART:{{ event_instance.start|date:"Ymd\THis" }}
DTEND:{{ event_instance.end|date:"Ymd\THis" }}
SUMMARY:{{ event_instance.title|linebreaksbr }}
DESCRIPTION:{{ event_instance.event.description|linebreaksbr }}
END:VEVENT
{% endfor %}
END:VCALENDAR