{
	"event_id": "{{ event_instance.event.pk }}",
	"id": "{{ event_instance.pk }}",
	"title": "{% if event_instance.event.canceled %}CANCELED: {% endif %}{{ event_instance.title|escapejs }}",
	"subtitle": null,
	"description": "{{ event_instance.event.description|escapejs }}",
	"location": "{{ event_instance.location.title|escapejs }}: {{ event_instance.location.room|escapejs }}",
	"location_url": "{{ event_instance.location.url|escapejs }}",
	"starts": "{{ event_instance.start|date:'r' }}",
	"ends": "{{ event_instance.end|date:'r' }}",
	"ongoing": {% if event_instance.event.has_instances or event_instance.start.date != event_instance.end.date or event_instance.start|date:"H:i" == "00:00" or event_instance.end|date:"H:i" == "23:59" %}"True"{% else %}"False"{% endif %}
}
