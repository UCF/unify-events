{% load core_tags %}{% if not event_instances %}BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//UNIFY
BEGIN:VTIMEZONE
TZID:America/New_York
X-LIC-LOCATION:America/New_York
BEGIN:DAYLIGHT
TZOFFSETFROM:-0500
TZOFFSETTO:-0400
TZNAME:EDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0400
TZOFFSETTO:-0500
TZNAME:EST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE
{% endif %}
BEGIN:VEVENT
UID:{{ event_instance.get_absolute_url }}
DTSTAMP:{{ event_instance.start|date:"Ymd\THis" }}
DTSTART:{{ event_instance.start|date:"Ymd\THis" }}
DTEND:{{ event_instance.end|date:"Ymd\THis" }}
LOCATION:{% if event_instance.location.pk %}{% include_esi 'location' event_instance.location.pk 'comboname-escapeics' %}{% else %}Virtual{% endif %}
SUMMARY:{% if event_instance.event.canceled %}CANCELED - {% endif %}{{ event_instance.title|escapeics }}
URL:{{ event_instance.get_absolute_url }}
DESCRIPTION:{{ event_instance.event.description.strip|escapeics|default:FALLBACK_EVENT_DESCRIPTION }}{% if event_instance.virtual_url %}\n\nVirtual Location URL: {{ event_instance.virtual_url }}{% endif %}
END:VEVENT
{% if not event_instances %}
END:VCALENDAR
{% endif %}
